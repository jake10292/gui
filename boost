repeat
    wait()
until game:IsLoaded()
if game.PlaceId == 6069993731 then
    wait(3)
    function notify(text)
        game.StarterGui:SetCore(
            "SendNotification",
            {
                Title = "~IMPORTANT~",
                Text = text,
                Icon = "",
                Duration = 200
            }
        )
    end
    wait()
    notify("This is the manual haki farm, it does not serverhop so be careful")
    for i, b in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
        if b.Name == "EagleVision" and b:IsA("Sound") and b.Parent.Whispers:IsA("Sound") then
            local Event = game:GetService("Players").LocalPlayer.PlayerGui[b.Parent.Name].use
            Event:FireServer()
            wait()
        end
    end
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-5420.25, 32.33, 1616.62)
    wait(3)
    spawn(
        function()
            while true do
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame =
                    CFrame.new(-5420.25, 30, 1616.62)
                wait(1)
                local Tool = "Meat"
                game.Players[game.Players.LocalPlayer.Name].Character.Humanoid:EquipTool(
                    game.Players[game.Players.LocalPlayer.Name].Backpack[Tool]
                )
                game.Workspace[game.Players.LocalPlayer.Name]:FindFirstChild("Meat"):Activate()
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame =
                    CFrame.new(-5416.97, 32.01, 1629.40)
                wait(2)
                for i, v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                    if v.Name == "Armament" and v:IsA("LocalScript") then
                        local string_1 = "Down"
                        local Target = game:GetService("Players").LocalPlayer.PlayerGui[v.Parent.Name].use
                        Target:FireServer(string_1)
                        wait(1)
                        local string_1 = "Down"
                        local Target = game:GetService("Players").LocalPlayer.PlayerGui[v.Parent.Name].use
                        Target:FireServer(string_1)
                        wait(6.8)
                    end
                end
            end
        end
    )
else
    game:GetService("TeleportService"):Teleport(6069993731, game.Players.LocalPlayer)
end
